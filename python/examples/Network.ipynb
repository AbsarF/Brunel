   "source": [
    "import pandas as pd\n",
    "import brunel\n",
    "\n",
    "nodes = pd.read_csv(\"data/sample_nodes.csv\")\n",
    "edges = pd.read_csv(\"data/sample_edges.csv\")"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 2,
   "metadata": {
    "collapsed": false
   },
   "outputs": [
