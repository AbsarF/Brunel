   "source": [
    "library(brunel)\n",
    "nodes <- read.csv('data/sample_nodes.csv', header=TRUE)\n",
    "edges <- read.csv('data/sample_edges.csv', header=TRUE)"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 2,
   "metadata": {
    "collapsed": false
   },
   "outputs": [
    {
     "data": {
      "text/html": [
       "<!--   ~ Copyright (c) 2015 IBM Corporation and others.   ~   ~ Licensed under the Apache License, Version 2.0 (the \"License\");   ~ You may not use this file except in compliance with the License.   ~ You may obtain a copy of the License at   ~   ~     http://www.apache.org/licenses/LICENSE-2.0   ~   ~ Unless required by applicable law or agreed to in writing, software   ~ distributed under the License is distributed on an \"AS IS\" BASIS,   ~ WITHOUT WARRANTIES OR CONDITIONS OF ANY KIND, either express or implied.   ~ See the License for the specific language governing permissions and   ~ limitations under the License.   -->  <link rel=\"stylesheet\" type=\"text/css\" href=\"/nbextensions/brunel_ext/BrunelBaseStyles.css\"> <link rel=\"stylesheet\" type=\"text/css\" href=\"/nbextensions/brunel_ext/sumoselect/sumoselect.css\"> <style>     #vis6b8f6cb8-c047-11e5-9a4e-4f1aea797fce.brunel .chart1 .element2 .element {\n",
       "\tfill-opacity: 1;\n",
       "\tsize: 20px;\n",
       "} </style>  <div id=\"controls6b8f6cb9-c047-11e5-9a4e-4f1aea797fce\" class=\"brunel\"/> <svg id=\"vis6b8f6cb8-c047-11e5-9a4e-4f1aea797fce\" width=\"800\" height=\"600\"></svg>  <script> require.config({     waitSeconds: 60,     paths: {         'd3': '//cdnjs.cloudflare.com/ajax/libs/d3/3.5.5/d3.min',         'BrunelD3': '/nbextensions/brunel_ext/BrunelD3',         'BrunelData': '/nbextensions/brunel_ext/BrunelData',         'BrunelEventHandlers': '/nbextensions/brunel_ext/BrunelEventHandlers',         'BrunelJQueryControlFactory': '/nbextensions/brunel_ext/BrunelJQueryControlFactory',         'SumoSelect': '/nbextensions/brunel_ext/sumoselect/jquery.sumoselect.min'     },     shim: {         'BrunelD3': {             exports: 'BrunelD3'         },         'BrunelData': {             exports: 'BrunelData'         },         'BrunelEventHandlers': {             exports: 'BrunelEventHandlers'         },         'BrunelJQueryControlFactory': {             exports: 'BrunelJQueryControlFactory'         }     }  });  require([\"d3\"], function(d3) { require([\"BrunelD3\", \"BrunelData\", \"BrunelEventHandlers\", \"BrunelJQueryControlFactory\", \"SumoSelect\"], function(BrunelD3, BrunelData,BrunelEventHandlers,BrunelJQueryControlFactory, SumoSelect) {     function  BrunelVis(visId) {\r\n",
       "  \"use strict\"; // Strict Mode\r\n",
       "  var datasets = [],                               // Array of datasets for the original data\r\n",
       "      pre = function(d, i) { return d },           // Default pre-process does nothing\r\n",
       "      post = function(d, i) { return d },          // Default post-process does nothing\r\n",
       "      transitionTime = 200,                        // Transition time for animations\r\n",
       "      charts = [],                                 // The charts in the system\r\n",
       "      vis = d3.select('#' + visId).attr('class', 'brunel');  // the SVG container\r\n",
       "\r\n",
       "  // Define chart #1 in the visualization //////////////////////////////////////////////////////////\r\n",
       "\r\n",
       "  charts[0] = function() {\r\n",
       "    var geom = BrunelD3.geometry(vis.node(), 0, 0, 0, 0, 0, 0, 0, 0),\r\n",
       "      elements = [];                               // Array of elements in this chart\r\n",
       "    var graph;                                     // Graph used for nodes and links\r\n",
       "\r\n",
       "    // Define groups for the chart parts ///////////////////////////////////////////////////////////\r\n",
       "\r\n",
       "    var chart = vis.append('g').attr('class', 'chart1')\r\n",
       "      .attr('transform','translate(' + geom.chart_left + ',' + geom.chart_top + ')');\r\n",
       "    var interior = chart.append('g').attr('class', 'interior')\r\n",
       "      .attr('transform','translate(' + geom.inner_left + ',' + geom.inner_top + ')')\r\n",
       "      .attr('clip-path', 'url(#clip_vis6b8f6cb8-c047-11e5-9a4e-4f1aea797fce_1)');\r\n",
       "    vis.append('clipPath').attr('id', 'clip_vis6b8f6cb8-c047-11e5-9a4e-4f1aea797fce_1').append('rect')\r\n",
       "      .attr('x', -1).attr('y', -1)\r\n",
       "      .attr('width', geom.inner_width+2).attr('height', geom.inner_height+2);\r\n",
       "\r\n",
       "    // Define element #1 ///////////////////////////////////////////////////////////////////////////\r\n",
       "\r\n",
       "    elements[0] = function() {\r\n",
       "      var original, processed,           // data sets passed in and then transformed\r\n",
       "        element, data,                   // Brunel element information and brunel data\r\n",
       "        d3Data, selection;               // D3 version of data and D3 selection\r\n",
       "      var elementGroup = interior.append('g').attr('class', 'element1'),\r\n",
       "        main = elementGroup.append('g').attr('class', 'main'),\r\n",
       "        labels = BrunelD3.undoTransform(elementGroup.append('g').attr('class', 'labels'), elementGroup);\r\n",
       "\r\n",
       "      function makeData() {\r\n",
       "        original = datasets[0];\r\n",
       "        processed = pre(original, 0);\r\n",
       "        processed = post(processed, 0);\r\n",
       "        var f0 = processed.field('From'),\r\n",
       "          f1 = processed.field('To'),\r\n",
       "          f2 = processed.field('#row');\r\n",
       "        var keyFunction = function(d) { return f0.value(d)+ '|' + f1.value(d) };\r\n",
       "        data = {\r\n",
       "          From:         function(d) { return f0.value(d.row) },\r\n",
       "          To:           function(d) { return f1.value(d.row) },\r\n",
       "          $row:         function(d) { return f2.value(d.row) },\r\n",
       "          From_f:       function(d) { return f0.valueFormatted(d.row) },\r\n",
       "          To_f:         function(d) { return f1.valueFormatted(d.row) },\r\n",
       "          $row_f:       function(d) { return f2.valueFormatted(d.row) },\r\n",
       "          _split:       function(d) { return 'ALL' },\r\n",
       "          _rows:        BrunelD3.makeRowsWithKeys(keyFunction, processed.rowCount())\r\n",
       "        };\r\n",
       "      }\r\n",
       "\r\n",
       "      // Build element from data ///////////////////////////////////////////////////////////////////\r\n",
       "\r\n",
       "      function build(transitionMillis) {\r\n",
       "        element = elements[0];\r\n",
       "        d3Data = graph.links;\r\n",
       "        selection = main.selectAll('*').data(d3Data, function(d) { return d.key});\r\n",
       "        selection.enter().append('line').attr('class',  'element edge');\r\n",
       "\r\n",
       "        BrunelD3.trans(selection,transitionMillis)\r\n",
       "          .attr('x1',function(d) { return d.source.x })\r\n",
       "          .attr('y1',function(d) { return d.source.y })\r\n",
       "          .attr('x2',function(d) { return d.target.x })\r\n",
       "          .attr('y2',function(d) { return d.target.y });\r\n",
       "\r\n",
       "        BrunelD3.trans(selection.exit(),transitionMillis/3)\r\n",
       "          .style('opacity', 0.5).each( function() {this.remove(); if (this.__label__) this.__label__.remove()});\r\n",
       "      }\r\n",
       "\r\n",
       "      return {\r\n",
       "        data:           function() { return processed },\r\n",
       "        internal:       function() { return data },\r\n",
       "        selection:      function() { return selection },\r\n",
       "        makeData:       makeData,\r\n",
       "        build:          build,\r\n",
       "        fields: {\r\n",
       "        }\r\n",
       "      };\r\n",
       "    }();\r\n",
       "\r\n",
       "    // Define element #2 ///////////////////////////////////////////////////////////////////////////\r\n",
       "\r\n",
       "    elements[1] = function() {\r\n",
       "      var original, processed,           // data sets passed in and then transformed\r\n",
       "        element, data,                   // Brunel element information and brunel data\r\n",
       "        d3Data, selection;               // D3 version of data and D3 selection\r\n",
       "      var elementGroup = interior.append('g').attr('class', 'element2'),\r\n",
       "        main = elementGroup.append('g').attr('class', 'main'),\r\n",
       "        labels = BrunelD3.undoTransform(elementGroup.append('g').attr('class', 'labels'), elementGroup);\r\n",
       "\r\n",
       "      function makeData() {\r\n",
       "        original = datasets[1];\r\n",
       "        processed = pre(original, 1);\r\n",
       "        processed = post(processed, 1);\r\n",
       "        var f0 = processed.field('Node'),\r\n",
       "          f1 = processed.field('#row');\r\n",
       "        var keyFunction = function(d) { return f0.value(d) };\r\n",
       "        data = {\r\n",
       "          Node:         function(d) { return f0.value(d.row) },\r\n",
       "          $row:         function(d) { return f1.value(d.row) },\r\n",
       "          Node_f:       function(d) { return f0.valueFormatted(d.row) },\r\n",
       "          $row_f:       function(d) { return f1.valueFormatted(d.row) },\r\n",
       "          _split:       function(d) { return f0.value(d.row) },\r\n",
       "          _rows:        BrunelD3.makeRowsWithKeys(keyFunction, processed.rowCount())\r\n",
       "        };\r\n",
       "      }\r\n",
       "      // Aesthetic Functions\r\n",
       "      var scale_color = d3.scale.ordinal()\r\n",
       "        .domain(['a', 'b', 'c', 'd', 'e', 'f', 'g'])\r\n",
       "        .range([ '#00538A', '#C10020', '#F4C800', '#007D34', '#803E75', '#FF6800', \r\n",
       "          '#817066', '#FFB300', '#F6768E', '#93AA00', '#53377A', '#FF8E00', '#B32851', \r\n",
       "          '#CEA262', '#FF7A5C', '#7F180D', '#593315', '#F13A13', '#232C16']);\r\n",
       "      var color = function(d) { return scale_color(data.Node(d)) };\r\n",
       "\r\n",
       "      // Build element from data ///////////////////////////////////////////////////////////////////\r\n",
       "\r\n",
       "      function build(transitionMillis) {\r\n",
       "        element = elements[1];\r\n",
       "        // Data structures for a network diagram\r\n",
       "        graph = BrunelData.diagram_Graph.make(processed, 'Node', elements[0].data(), 'From', 'To');\r\n",
       "\r\n",
       "        // Initial Circle Layout\r\n",
       "        var h = geom.inner_width/2, v = geom.inner_height/2, a, i, N = graph.nodes.length;\r\n",
       "        for(i=0; i<N; i++) { a = Math.PI*2*i/N; graph.nodes[i].x = h + h*Math.cos(a)/2; graph.nodes[i].y = v + v*Math.sin(a)/2 }\r\n",
       "\r\n",
       "        var labeling = {\r\n",
       "          method: 'box', \r\n",
       "          fit: false,\r\n",
       "          content: function(d) {\r\n",
       "            return d.row == null ? null : data.Node_f(d)\r\n",
       "          }\r\n",
       "        };\r\n",
       "        main.attr('class', 'diagram network');\r\n",
       "        d3Data = graph.nodes;\r\n",
       "        selection = main.selectAll('*').data(d3Data, function(d) { return d.key});\r\n",
       "        selection.enter().append('circle').attr('class',  'element point');\r\n",
       "        BrunelD3.trans(selection,transitionMillis)\r\n",
       "          .attr('r', 20.0);\r\n",
       "        selection.style('fill', function(d) { if (d == null || d.row == null) return; return color(d) });\r\n",
       "        BrunelD3.label(selection, labels, labeling, 0);\r\n",
       "\r\n",
       "        BrunelD3.trans(selection.exit(),transitionMillis/3)\r\n",
       "          .style('opacity', 0.5).each( function() {this.remove(); if (this.__label__) this.__label__.remove()});\r\n",
       "      }\r\n",
       "\r\n",
       "      return {\r\n",
       "        data:           function() { return processed },\r\n",
       "        internal:       function() { return data },\r\n",
       "        selection:      function() { return selection },\r\n",
       "        makeData:       makeData,\r\n",
       "        build:          build,\r\n",
       "        fields: {\r\n",
       "          color:        ['Node']\r\n",
       "        }\r\n",
       "      };\r\n",
       "    }();\r\n",
       "\r\n",
       "    function build(time) {\r\n",
       "      var first = elements[0].data() == null;\r\n",
       "      if (first) time = 0; // No transition for first call\r\n",
       "      if (first || time>0) {\r\n",
       "        elements[1].makeData();\r\n",
       "        elements[0].makeData();\r\n",
       "      }\r\n",
       "      elements[1].build(time);\r\n",
       "      elements[0].build(time);\r\n",
       "      if (first) BrunelD3.network(d3.layout.force(), graph, elements[1], elements[0], geom);\r\n",
       "    }\r\n",
       "\r\n",
       "    // Expose the following components of the chart\r\n",
       "    return { build : build, elements : elements };\r\n",
       "  }();\r\n",
       "\r\n",
       "  function setData(rowData, i) { datasets[i||0] = BrunelD3.makeData(rowData) }\r\n",
       "  function updateAll(time)     { charts.forEach(function(x) {x.build(time || 20)}) }\r\n",
       "  function buildAll() {\r\n",
       "    for (var i=0;i<arguments.length;i++) setData(arguments[i], i);\r\n",
       "    updateAll(transitionTime);\r\n",
       "  }\r\n",
       "\r\n",
       "  return {\r\n",
       "    dataPreProcess:     function(f) { if (f) pre = f; return pre },\r\n",
       "    dataPostProcess:    function(f) { if (f) post = f; return post },\r\n",
       "    data:               function(d,i) { if (d) setData(d,i); return datasets[i||0] },\r\n",
       "    visId:              visId,\r\n",
       "    build:              buildAll,\r\n",
       "    rebuild:            updateAll,\r\n",
       "    charts:             charts\r\n",
       "  }\r\n",
       "}\r\n",
       "\r\n",
       "// Data Tables /////////////////////////////////////////////////////////////////////////////////////\r\n",
       "\r\n",
       "var table1 = {\r\n",
       "   names: ['From', 'To'], \r\n",
       "   options: ['string', 'string'], \r\n",
       "   rows: [['a', 'b'], ['a', 'c'], ['a', 'd'], ['b', 'c'], ['a', 'e'], ['e', 'f']]\r\n",
       "};\r\n",
       "var table2 = {\r\n",
       "   names: ['Node'], \r\n",
       "   options: ['string'], \r\n",
       "   rows: [['a'], ['b'], ['c'], ['d'], ['e'], ['f'], ['g']]\r\n",
       "};\r\n",
       "\r\n",
       "// Call Code to Build the system ///////////////////////////////////////////////////////////////////\r\n",
       "\r\n",
       "var v = new BrunelVis('vis6b8f6cb8-c047-11e5-9a4e-4f1aea797fce');\r\n",
       "v.build(table1, table2);\r\n",
       " }); }); </script>"
      ]
     },
     "metadata": {},
     "output_type": "display_data"
    }
   ],
   "source": [
    "brunel (\"data('edges') key(From,To) edge + data('nodes') network color(Node) key(Node) legends(none) style('fill-opacity:1;size:20px') label(Node)\")"
   ]
  },
